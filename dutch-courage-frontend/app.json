{
  "expo": {
    "name": "dutch-courage-frontend",
    "slug": "dutch-courage-frontend",
    "scheme": "dutch-courage-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      // "image": "./assets/splash.png",
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/adaptive-icon.png",
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tanzeemrahat.dutchcouragefrontend",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBUdvJx-4tLkS6X8fuOiJnTHFyQ7oc77QM"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      // "favicon": "./assets/favicon.png",
      "favicon": "./assets/icon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "94ad761d-8ec0-4f10-bb04-57aacaa8aba9"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
